{
  "name": "MongoDB Timeseries Workshop",
  "image": "mcr.microsoft.com/devcontainers/python:3.10",
  "hostRequirements": {
      "cpus": 4
  },
  "postCreateCommand": "pip install pymongo matplotlib pandas",
  "forwardPorts": [
    27017
  ],
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter",
          "ms-tools.vscode-jupyter",
          "mongodb.mongodb-vscode"
      ]
    }
  }
}

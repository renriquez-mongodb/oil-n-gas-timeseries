{
  "name": "MongoDB Timeseries Workshop",
  "dockerComposeFile": "docker-compose.yml",
  "hostRequirements": {
      "cpus": 4
  },
  "updateContentCommand": "python3 -m pip install -r requirements.txt",
  "forwardPorts": [
    27017
  ],
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter",
          "ms-tools.vscode-jupyter",
          "mongodb.mongodb-vscode"
      ]
    }
  }
}
